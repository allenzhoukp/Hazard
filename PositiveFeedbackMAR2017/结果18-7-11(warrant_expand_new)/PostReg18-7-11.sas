		if	n_day	=	445	then xbeta_sec_date_maturity = xbeta_sec_date +	0.01894	;
		if	n_day	=	446	then xbeta_sec_date_maturity = xbeta_sec_date +	0.02839	;
		if	n_day	=	447	then xbeta_sec_date_maturity = xbeta_sec_date +	0.16157	;
		if	n_day	=	448	then xbeta_sec_date_maturity = xbeta_sec_date +	0.29978	;
		if	n_day	=	449	then xbeta_sec_date_maturity = xbeta_sec_date +	0.09083	;
		if	n_day	=	450	then xbeta_sec_date_maturity = xbeta_sec_date +	0	;

run;

/* Drop NA (not involved in the regression)	and calc xbeta_zero.
   xbeta_zero is the X'beta
   when lag1_return and D1 keeps ZERO VALUES*/

data warrant_expand_new;
	set warrant_expand_new;
	if n_day = . then delete;
	if date = . then delete;
	if xbeta_init = . then delete;
	xbeta = xbeta_sec_date_maturity;
	drop xbeta_sec xbeta_sec_date xbeta_sec_date_maturity;

	/* 18-7-13-14:54 NOTE GU says that last_return and last_num should not be included
	in the 'set as zero' variables. Remain checked. */

	xbeta_zero = xbeta -
	(
		hold_return	*	0.1021	+
		last_return	*	0.07607	+
		other_return	*	0.05356	+
		hold_num	*	0.01864	+
		last_num	*	-0.05131	+
		other_num	*	-0.00906
	);

run;


/* baseline dataset plays the role of LAMBDA.  */

/* Copied from date1_lambda 3.19 2017.txt:
   Preprocess real baseline into lambda:
   	cumulative lambda = cumhaz / exp(xbeta)
   	and take a diff.

   Merge lambda and x'beta on a.end = b.end since lambda = lambda(end) */

data baseline_new_lambda;
	set warrant.baseline_expandNew;
	survival_test = exp(-cumhaz);
	cumlambda = cumhaz / exp(xbeta);
	lagcumlambda = lag(cumlambda);
	lambda = cumlambda - lagcumlambda;
run;

proc sql;
create table prediction_new as
	select distinct a.*, b.lambda as lambda
		from warrant_expand_new a left join baseline_new_lambda b
			on a.end = b.end;
quit;

/* NOTE All NA lambdas set to ZERO!

   calc probability. */

data prediction_new;
	set prediction_new;
	if lambda = . then lambda = 0;
	p = 1 - exp(-lambda * exp(xbeta));
	p_zero = 1 - exp(-lambda * exp(xbeta_zero));
run;


/* Obtain warrant_start as first transaction of a pseudocycle.
   NOTE in warrant_start we've got securitycode as character. Convert that first.

   previous_start_amount:
   Mean of first transaction's amount of all Previous psudocycles,
   FOR ALL securitycodes.
   (So, this should be Q in that formula. Only FIRST transaction of a psudocycle counts.)

   - Requires sorting */

data warrant_start;
   	set warrant.warrant_start;
	format a best12.;
	a = securitycode;
	drop securitycode;
	rename a = securitycode;
	run;
run;

proc sort data=prediction_new out=prediction_new;
   	by fundacctnum securitycode date;
run;
proc sort data=warrant_start out=warrant_start;
   	by fundacctnum securitycode date;
run;

proc sql;
create table prediction_new as
	select distinct a.*, mean(b.amount) as avg_allcycles_start_amount
		from prediction_new a left join warrant_start b
			on a.fundacctnum = b.fundacctnum and b.date < a.date
		group by a.fundacctnum, a.securitycode, a.date;
quit;

/* pre amount = prob * prev_start_amount (p * q) */
data prediction_new;
	set prediction_new;
	amount = p * avg_allcycles_start_amount;
	zero_amount = p_zero * avg_allcycles_start_amount;
run;

/* sum over Fundacctnums */
proc sql;
create table prediction_new as
	select distinct
			securitycode, date,
			sum(p) as p_num,
			sum(p_zero) as p_zero_num,
			sum(amount) as amount,
			sum(zero_amount) as zero_amount
		from prediction_new
		group by securitycode, date
		order by securitycode, date;
quit;

/* Get pre amount */
data warrant.prediction_warrant_expand_new;
	set prediction_new;
	diff = amount - zero_amount;
run;
